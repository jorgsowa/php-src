--TEST--
bcdiv() function - tests with number zero
--EXTENSIONS--
bcmath
--INI--
bcmath.scale=0
--FILE--
<?php

$basicNumbers = ["0", "0.00", "-0", "-0.00"];
$allTestNumbers = ["15", "-15", "1", "-9", "14.14", "-16.60", "0.15", "-0.01", "15151324141414.412312232141241", "-132132245132134.1515123765412", "141241241241241248267654747412", "-149143276547656984948124912", "0.1322135476547459213732911312", "-0.123912932193769965476541321"];
$scales = [0,10];

foreach($scales as $scale) {
	foreach($basicNumbers as $firstNumber) {
    	echo "Number \"$firstNumber\" (scale $scale)\n";
		foreach($allTestNumbers as $secondNumber) {
			echo str_pad($firstNumber, 6, ' ', STR_PAD_LEFT), " mod " , str_pad($secondNumber,30), ' = ', bcmod($firstNumber, $secondNumber, $scale),"\n";
		}
		echo "\n";
	}
}

?>
